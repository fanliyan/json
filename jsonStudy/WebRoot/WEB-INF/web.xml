<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
  <servlet>
    <servlet-name>ajaxLoginServlet</servlet-name>
    <servlet-class>com.servlet.ajaxLoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ajaxLoginServlet</servlet-name>
    <url-pattern>/ajaxLoginServlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
  		  <servlet-name>ajaxShowFileServlet</servlet-name>
  		  <servlet-class>com.servlet.ajaxShowFileServlet</servlet-class>
  </servlet>
  <servlet-mapping>
             <servlet-name>ajaxShowFileServlet</servlet-name>
             <url-pattern>/ajaxShowFileServlet</url-pattern>
  </servlet-mapping>

</web-app>